<h1> producto</h1>
<%=form_with model: @product do |form|%>
    <%@product.errors.full_messages.each do |error|%>
        <%=error%>
    <%end%>
    <div>
        <%=form.label :nombre%>
        <%=form.text_field :nombre%>
    </div>
    <div>
        <%=form.label :descripcion%>
        <%=form.text_area :descripcion%>
    </div>
    <div>
        <%=form.label :precio%>
        <%=form.number_field :precio%>
    </div>
    <%=form.submit%>
<%end%>